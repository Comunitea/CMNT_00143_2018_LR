<odoo>
     <!-- account.payment.order inherit form view -->
     <record id="account_payment_order_form_add_ignored" model="ir.ui.view">
         <field name="name">account.payment.order.inherit.view.form.add.ignored</field>
         <field name="model">account.payment.order</field>
         <field name="inherit_id" ref="account_payment_order.account_payment_order_form"/>
         <field name="arch" type="xml">
            <notebook position="inside">
                <page name="ignored-lines" string="Ignored Transactions">
                    <field name="ignored_payment_line_ids"/>
                </page>
            </notebook>
         </field>
     </record>

</odoo>

