    <data>

        <record id="menu_my_carts" model="website.menu">
            <field name="name">My Carts</field>
            <field name="url">/my/carts</field>
            <field name="parent_id" ref="website.main_menu"/>
            <field name="sequence" type="int">20</field>
            <field name="website_id" ref="website.default_website"/>
            <field name="website_published">True</field>
            <field name="for_retailers">True</field>
            <field name="for_customers">False</field>
            <field name="top_menu_retailers">True</field>
        </record>

