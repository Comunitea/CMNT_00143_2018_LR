<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <!-- Add custom styles -->
    <template id="rias_website_assets_frontend" inherit_id="website.assets_frontend">
        <xpath expr="link[last()]" position="after">
            <link href="/theme_rias_multi_cart/static/css/page_shop.less" rel="stylesheet" type="text/less"/>
        </xpath>

        <!--Add custom scripts-->
        <xpath expr="script[last()]" position="after">
            <script type="text/javascript" src="/theme_rias_multi_cart/static/js/shop.js" />
        </xpath>
    </template>
    
</odoo>