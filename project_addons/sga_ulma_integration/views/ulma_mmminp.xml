<?xml version="1.0" encoding="utf-8"?>
<odoo>
    
    <record model="ir.ui.view" id="ulma_mmminp_form">
        <field name="name">ulma.mmminp.form</field>
        <field name="model">ulma.mmminp</field>
        <field name="arch" type="xml">
            <form string="Ulma mmminp">
                <sheet>
                    <group>
                        <group>
                            <field name="id"/>
                            <field name="mmmres"/>
                            <field name="mmmges"/>
                            <field name="mmmcmdref"/>
                            <field name="mmmexpordref"/>
                            <field name="mmmartref"/>
                        </group>
                        <group>
                            <field name="mmmsesid"/>
                            <field name="mmmacccolcod"/>
                            <field name="mmmresmsj"/>
                            <field name="mmmcanuni"/>
                            <field name="momcre"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record model="ir.ui.view" id="ulma_mmminp_search">
        <field name="name">ulma.mmminp.search</field>
        <field name="model">ulma.mmminp</field>
        <field name="arch" type="xml">
                <search string="Ulma mmminp">
                    <field name="mmmres" string="Estado"/>
                    <field name="mmmacccolcod" string="Contenedor"/>
                    <field name="mmmartref"/>
                    <field name="momcre"/>
                    <field name="mmmexpordref" string="Picking"/>
                </search>
            </field>
    </record>

    <record model="ir.ui.view" id="ulma_mmminp_tree">
        <field name="name">ulma.mmminp.tree</field>
        <field name="model">ulma.mmminp</field>
        <field name="arch" type="xml">
            <tree string="Ulma mmminp">
                <field name="id" />
                <field name="mmmexpordref" string="Picking"/>
                <field name="mmmartref" string="Referencia"/>
                <field name="mmmres" string="Estado"/>
                <field name="mmmacccolcod" string="Contenedor"/>
                <field name="mmmresmsj" string="Mensaje"/>
                <field name="momcre" string="Fecha"/>
            </tree>
        </field>
    </record>

    <record id="action_ulma_mmminp_view" model="ir.actions.act_window">
        <field name="name">Ulma mmminp</field>
        <field name="res_model">ulma.mmminp</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
        <field name="view_ids" eval="[(5, 0, 0),
                (0, 0, {'view_mode': 'tree', 'view_id': ref('sga_ulma_integration.ulma_mmminp_tree')}),
                (0, 0, {'view_mode': 'form', 'view_id': ref('sga_ulma_integration.ulma_mmminp_form')})]"/>
        <field name="search_view_id" ref="ulma_mmminp_search"/>
    </record>

    <menuitem id="menu_sga_ulma_views_mmminp" name="Ulma MMMINP" parent="menu_sga_ulma" action="sga_ulma_integration.action_ulma_mmminp_view"  groups="stock.group_stock_manager"/>

</odoo>