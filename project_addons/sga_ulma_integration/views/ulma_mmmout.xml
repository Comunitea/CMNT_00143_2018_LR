        <field name="model">ulma.mmmout</field>
        <field name="arch" type="xml">
            <tree string="Ulma MMMOUT">
                <field name="id" />
                <field name="mmmcmdref" string="Reference"/>
                <field name="mmmbatch" string="Batch"/>
                <field name="mmmcntdorref" string="Package"/>
            </tree>
        </field>
    </record>

    <record id="action_ulma_mmmout_view" model="ir.actions.act_window">
        <field name="name">Ulma MMMOUT</field>
        <field name="res_model">ulma.mmmout</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
        <field name="view_ids" eval="[(5, 0, 0),
