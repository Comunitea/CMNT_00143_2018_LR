<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="website_menu_lasrias_menu_tree" model="ir.ui.view">
            <field name="name">website_menu_lasrias_menu_tree</field>
            <field name="model">website.menu</field>
            <field name="inherit_id" ref="website.menu_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='sequence']" position="before">
                    <field name="active"/>
                </xpath>
                <xpath expr="//field[@name='url']" position="after">
                    <field name="menu_icon"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>