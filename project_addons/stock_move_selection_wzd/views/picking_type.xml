<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_picking_type_kanaban_form" model="ir.ui.view">
        <field name="name">stock.picking.type.form.selection</field>
        <field name="model">stock.picking.type</field>
        <field name="inherit_id" ref="stock.view_picking_type_form"/>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <field name="show_reserved" position="after">
                <field name="num_error_day"/>
            </field>
            <field name="warehouse_id" position="after">
                <field name="description"/>
                <field name="sga_integrated"/>
                <field name="sga_integration_type" attrs="{'invisible': [('sga_integrated','=',False)]}" />

            </field>

            <xpath expr="//group[@name='logistics']" position="after">
                 <div></div>
                 <group string="Kanban conf.">
                    <field name="visible_kanban_conf"/>
                    <field name="ratio_object"/>
                 </group>

                    <group string="Albaranes y Reservas" attrs="{'invisible': [('visible_kanban_conf','=', False)]}">
                        <field name="need_batch_delivery"/>
                        <field name="need_batch_picking"/>
                        <field name="need_sale_order"/>
                        <field name="need_purchase_order"/>

                        <field name="visible_count_picking"/>
                        <field name="visible_count_picking_waiting"/>
                        <field name="visible_count_picking_ready"/>
                        <field name="visible_count_picking_late"/>
                        <field name="visible_count_picking_backorders"/>

                        <field name="visible_count_pick_to_batch"/>
                        <field name="visible_count_batch_ready"/>

                    </group>
                    <group string="Movimientos" attrs="{'invisible': [('visible_kanban_conf','=', False)]}">
                        <field name="visible_bargraph"/>
                        <field name="visible_ratio"/>
                        <field name="visible_count_move_ready"/>
                        <field name="visible_count_move_todo_today"/>
                        <field name="visible_count_move_waiting"/>
                        <field name="visible_count_move_done"/>
                        <field name="visible_count_move_late"/>
                        <field name="visible_count_move_backorders"/>
                        <field name="visible_count_move_to_pick"/>
                        <field name="visible_count_move_unpacked"/>
                        <field name="visible_last_done_move"/>
                    </group>

            </xpath>




        </field>
    </record>


    <record id="stock_picking_type_kanban_moves" model="ir.ui.view">
            <field name="name">stock.picking.type.kanban.moves</field>
            <field name="model">stock.picking.type</field>
            <field name="arch" type="xml">
                <kanban class="oe_background_grey o_kanban_dashboard o_emphasize_colors o_account_kanban" create="0">
                    <field name="color"/>
                    <field name="code"/>

                    <field name="last_done_move"/>
                    <field name="visible_last_done_move"/>

                    <field name="count_move_ready"/>
                    <field name="visible_count_move_ready"/>

                    <field name="count_move_draft"/>

                    <field name="count_move_waiting"/>
                    <field name="visible_count_move_waiting"/>

                    <field name="count_move_late"/>
                    <field name="visible_count_move_late"/>

                    <field name="count_move_backorders"/>
                    <field name="visible_count_move_backorders"/>

                    <field name="count_move_to_pick"/>
                    <field name="visible_count_move_to_pick"/>


                    <field name="count_pick_to_batch"/>
                    <field name="visible_count_pick_to_batch"/>

                    <field name="count_batch_ready"/>
                    <field name="count_batch"/>
                    <field name="visible_count_batch_ready"/>

                    <field name="count_picking_ready"/>
                    <field name="visible_count_picking_ready"/>


                    <field name="count_move_unpacked"/>
                    <field name="visible_count_move_unpacked"/>


                    <field name="count_picking_waiting"/>
                    <field name="visible_count_picking_waiting"/>

                    <field name="count_picking_late"/>
                    <field name="visible_count_picking_late"/>

                    <field name="count_picking_backorders"/>
                    <field name="count_picking_pasaran"/>
                    <field name="visible_count_picking_backorders"/>

                    <field name="visible_bargraph"/>
                    <field name="visible_ratio"/>

                    <field name="count_sga_done"/>
                    <field name="count_sga_undone"/>
                    <field name="count_sga_send"/>
                    <field name="count_sga_error"/>
                    <field name="actual_picking_ids"/>
                    <field name="sga_integrated"/>
                    <field name="group_code"/>
                    <field name="context_domain"/>
                    <field name="shipping_type"/>
                    <field name="delivery_route_path_id"/>
                    <field name="kanban_dashboard_graph"/>

                    <field name="need_batch_delivery"/>
                    <field name="need_batch_picking"/>
                    <field name="need_sale_order"/>
                    <field name="need_purchase_order"/>

                    <field name="ratio_object"/>
                    <field name="rate_sga_undone"/>
                    <field name="rate_packed_undone"/>
                    <field name="rate_undone"/>
                    <field name="rate_late"/>


                    <field name="description"/>
                    <field name="count_move_pasaran"/>
                    <templates>
                        <t t-name="kanban-box">
                            <div t-if="['incoming', 'outgoing', 'internal'].indexOf(record.code.raw_value) &gt; -1"
                                 t-attf-class="#{kanban_color(record.color.raw_value)}" name="stock_picking">
                                <div>
                                    <div t-attf-class="o_kanban_card_header">
                                        <div class="o_kanban_card_header_title">
                                            <div class="o_primary">
                                                <span t-if="record.sga_integrated.raw_value" class="badge">
                                                    <i class="fa fa-fw fa-rss-square"/>
                                                </span>
                                                <a type="object" name="get_stock_move_action_move_type">
                                                    <field name="name"/>
                                                </a>
                                            </div>
                                            <div class="o_secondary">
                                                <field class="text-primary" name="warehouse_id"/>:
                                                <field class="o_secondary" name="description"/>
                                            </div>
                                        </div>
                                        <div class="o_kanban_manage_button_section">
                                            <a class="o_kanban_manage_toggle_button" href="#"><i class="fa fa-ellipsis-v"/></a>
                                        </div>
                                    </div>

                                    <div class="container o_kanban_card_content">
                                        <div class="row">
                                            <div class="col-xs-8 o_kanban_primary_left">
                                                <!--div name='sga'>
                                                    <div class="row col-xs-9"
                                                         title="Reservas de pedidos: Preparados (Retrasados)"
                                                         t-if="record.visible_count_picking_ready.raw_value == true">
                                                        <button
                                                                class="btn btn-primary" name="get_action_picking_tree_ready" type="object">
                                                                <span><t t-esc="record.count_picking_ready.value"/></span>
                                                                <span t-if="record.count_picking_late.raw_value &gt; 0 and False">(<t t-esc="record.count_picking_late.value"/>)</span>
                                                                <span t-if="record.group_code.raw_value =='incoming'">Recepciones</span>
                                                                <span t-if="record.group_code.raw_value =='outgoing'">Reservas de pedidos</span>
                                                                <span t-if="record.group_code.raw_value =='picking'">Batch</span>
                                                                <span t-if="record.group_code.raw_value =='internal'">Internos </span>
                                                                <span t-if="record.group_code.raw_value =='location'">Ubicaciones </span>
                                                                <span t-if="record.group_code.raw_value =='reposition'">Reposiciones </span>
                                                                <span t-if="record.group_code.raw_value =='other'">Albaranes </span>
                                                        </button>
                                                    </div>
                                                </div-->

                                                <div name="sga_state" t-if="record.sga_integrated.raw_value == true">
                                                     <div class="row">
                                                        <div class="col-xs-9 text-muted" >
                                                            <h3 class="text-muted">Movimientos SGA</h3>
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="col-xs-6 text-primary" title="Movimientos de SGA (Totales)">
                                                            <a name="get_action_tree" type="object" context="{'dest_model': 'stock.move',
                                                                                                              'move_domain': 'count_sga_send',
                                                                                                              'default_domain': [('sga_integrated', '=', True)],
                                                                                                              'search_default_waiting_sga': 1}">
                                                                Pendientes de SGA
                                                            </a>
                                                        </div>
                                                        <div class="col-xs-3">
                                                            <span><t t-esc="record.count_sga_undone.value"/> de <t t-esc="record.count_sga_send.value"/></span>
                                                        </div>
                                                    </div>
                                                </div>

                                                <div name="pack_state" t-if="record.visible_count_move_unpacked.raw_value == true">
                                                     <div class="row">
                                                        <div class="col-xs-9 text-muted" >
                                                            <h3 class="text-muted">Estado Empaquetado</h3>
                                                        </div>
                                                    </div>
                                                    <div t-if="record.visible_count_move_unpacked.raw_value == true" class="row">
                                                        <div class="col-xs-6 text-primary" title="Para empaquetar">
                                                            <a name="get_action_tree" type="object" context="{'dest_model': 'stock.move',
                                                                                                              'move_domain': 'count_move_ready',
                                                                                                              'search_default_with_no_package': 1}">

                                                                Para empaquetar
                                                            </a>
                                                        </div>
                                                        <div class="col-xs-3">
                                                            <span><t t-esc="record.count_move_unpacked.value"/> de <t t-esc="record.count_move_ready.value"/></span>
                                                        </div>
                                                    </div>
                                                </div>

                                                <div name="batch_delivery" t-if="record.need_batch_delivery.raw_value == true">
                                                     <div class="row">
                                                        <div class="col-xs-9 text-muted" >
                                                            <h3 class="text-muted">Ordenes de carga</h3>
                                                        </div>
                                                    </div>
                                                </div>

                                                <div name="batch_picking" t-if="record.need_batch_picking.raw_value == true">
                                                     <div class="row">
                                                        <div class="col-xs-9 text-muted" >
                                                            <h3 class="text-muted">Batchs</h3>
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="col-xs-6 text-primary" title="Pendientes (Todos)">
                                                            <a name="get_action_tree" type="object" context="{'dest_model': 'stock.batch.picking',
                                                                                                              'batch_domain': 'count_all_batch',
                                                                                                              'search_default_last_day': 1,
                                                                                                              'search_default_todo': 1}">
                                                                Pendientes
                                                            </a>
                                                        </div>
                                                        <div class="col-xs-3">
                                                            <span><t t-esc="record.count_batch_ready.value"/> de <t t-esc="record.count_batch.value"/></span>
                                                        </div>
                                                    </div>
                                                </div>


                                                <div name="sale_orders" t-if="record.need_sale_order.raw_value == true">
                                                    <div class="row">
                                                    <div class="col-xs-9 text-muted" >
                                                        <h3 class="text-muted">Pedidos</h3>
                                                    </div>
                                                      </div>
                                                    <div t-if="record.visible_count_picking_ready.raw_value == true" class="row">
                                                        <div class="col-xs-6 text-primary" title="Pedidos pendientes">
                                                            <a name="get_action_tree" type="object" context="{'dest_model': 'stock.picking',
                                                                                                              'pick_domain': 'count_picking',
                                                                                                              'search_default_available': 1,
                                                                                                              'search_default_by_shipping_type':1,
                                                                                                              'search_default_by_delivery_path_route_id':1,
                                                                                                              'search_default_by_partner_id':1,}">
                                                                Todos
                                                            </a>
                                                        </div>
                                                        <div class="col-xs-3">
                                                            <span><t t-esc="record.count_picking_ready.value"/> de <t t-esc="record.count_picking.value"/></span>
                                                        </div>
                                                    </div>

                                                    <div t-if="record.need_batch_picking.raw_value == true" class="row">
                                                        <div class="col-xs-6 text-primary" title="Pedidos sin batch">
                                                            <a name="get_action_tree" type="object" context="{'dest_model': 'stock.picking',
                                                                                                              'pick_domain': 'count_picking',
                                                                                                              'search_default_to_batch': 1,
                                                                                                              'search_default_available': 1,
                                                                                                              'search_default_by_shipping_type':1,
                                                                                                              'search_default_by_delivery_path_route_id':1,
                                                                                                              'search_default_by_partner_id':1,}">
                                                                Sin batch
                                                            </a>
                                                        </div>
                                                        <div class="col-xs-3">
                                                            <span><t t-esc="record.count_pick_to_batch.value"/></span>
                                                        </div>
                                                    </div>
                                                </div>

                                                <div name="moves" t-if="record.need_purchase_order.raw_value == true">
                                                    <div class="row">
                                                        <div class="col-xs-9 text-muted" >
                                                            <h3 class="text-muted">Movimientos</h3>
                                                        </div>
                                                    </div>
                                                     <div class="col-xs-6 text-primary" title="Movimientos de SGA (Totales)">
                                                            <a name="get_action_tree" type="object" context="{'dest_model': 'stock.move',
                                                                                                              'move_domain': 'count_sga_send',
                                                                                                              'default_domain': [('sga_integrated', '=', True)],
                                                                                                              'search_default_waiting_sga': 1}">
                                                                Pendientes de SGA
                                                            </a>
                                                        </div>
                                                        <div class="col-xs-3">
                                                            <span><t t-esc="record.count_sga_undone.value"/>(<t t-esc="record.count_sga_send.value"/>)</span>
                                                        </div>
                                                    <div t-if="record.visible_count_move_unpacked.raw_value == true" class="row">
                                                        <div class="col-xs-6 text-primary" title="Para empaquetar">
                                                            <a name="get_action_tree" type="object" context="{'dest_model': 'stock.move',
                                                                                                              'move_domain': 'count_move_ready',
                                                                                                              'search_default_with_no_package': 1}">

                                                                Para empaquetar
                                                            </a>
                                                        </div>
                                                        <div class="col-xs-3">
                                                            <span><t t-esc="record.count_move_unpacked.value"/> de <t t-esc="record.count_move_ready.value"/></span>
                                                        </div>
                                                    </div>

                                                    <div t-if="record.visible_count_move_to_pick.raw_value == true" class="row">
                                                        <div class="col-xs-6 text-primary" title="Para empaquetar">
                                                            <a name="get_action_tree" type="object" context="{'dest_model': 'stock.move',
                                                                                                              'move_domain': 'count_move_ready',
                                                                                                              'search_default_to_batch': 1}">

                                                                Para albaranar
                                                            </a>
                                                        </div>
                                                        <div class="col-xs-3">
                                                            <field name="count_move_to_pick"/>
                                                        </div>
                                                    </div>

                                                    <div t-if="record.visible_count_move_ready.raw_value == true" class="row">
                                                        <div class="col-xs-6 text-primary" title="Movimientos que ya tienen reserva total o parcial y pueden ser procesados">
                                                            <a name="get_action_tree" type="object" context="{'dest_model': 'stock.move',
                                                                                                              'move_domain': 'count_move_ready',
                                                                                                              }">

                                                                Preparados
                                                            </a>
                                                        </div>
                                                        <div class="col-xs-3">
                                                            <field name="count_move_ready"/>
                                                        </div>
                                                    </div>

                                                    <div t-if="record.visible_count_move_waiting == true" class="row">
                                                        <div class="col-xs-6 text-warning" title="Movimientos sin stock o pendientes de otro movimiento">

                                                            <a name="get_action_tree" type="object" context="{'dest_model': 'stock.move',
                                                                                                              'move_domain': 'count_move_waiting',
                                                                                                              }">

                                                                En espera
                                                            </a>
                                                        </div>
                                                        <div class="col-xs-3">
                                                            <field name="count_move_waiting"/>
                                                        </div>
                                                    </div>

                                                    <div t-if="record.visible_count_move_late.raw_value == true" class="row">
                                                        <div class="col-xs-6 text-warning" title="Movimientos sin stock o pendientes de otro movimiento">

                                                            <a class="text-danger" name="get_action_tree" type="object" context="{'dest_model': 'stock.move',
                                                                                                              'move_domain': 'count_move_late',
                                                                                                              }">

                                                                Retrasados
                                                            </a>
                                                        </div>
                                                        <div class="col-xs-3">
                                                            <field name="count_move_late"/>
                                                        </div>
                                                    </div>

                                                    <div t-if="record.visible_count_move_backorders.raw_value == true" class="row">
                                                         <div class="col-xs-6 text-warning" title="Movimientos sin stock o pendientes de otro movimiento">

                                                            <a name="get_action_tree" type="object" context="{'dest_model': 'stock.move',
                                                                                                              'move_domain': 'count_move_backorders',
                                                                                                              }">

                                                                Back Orders
                                                            </a>
                                                        </div>
                                                        <div class="col-xs-3">
                                                            <field name="count_move_backorders"/>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                            <div t-if="record.visible_ratio.raw_value == true" class="row col-xs-4 text-center">
                                                <div t-if="record.ratio_object.raw_value == 'stock.move'">
                                                    <span>
                                                        <a type="object"
                                                            name="get_action_tree" title="Movimientos programados para hoy o realizados hoy"
                                                            context="{  'dest_model': 'stock.move',
                                                                        'move_domain': 'count_move_todo_today',
                                                                        'search_default_last_day': 1,
                                                                        'search_default_todo': 1}">
                                                        <field name="count_move_todo_today"/>
                                                        </a>
                                                    </span>
                                                </div>
                                                <div t-if="record.ratio_object.raw_value == 'stock.picking'">
                                                    <span>
                                                        <a type="object" name="get_action_tree" title="Ordenes programados para hoy o realizados hoy"
                                                            context="{'dest_model': 'stock.picking',
                                                            'pick_domain': 'count_picking',
                                                            'search_default_last_day': 1,
                                                            'search_default_todo': 1}">
                                                            <field name="count_picking"/>
                                                        </a>
                                                    </span>
                                                </div>

                                                    <div t-if="record.visible_count_move_unpacked.raw_value == true" >
                                                        <span class="o_label">
                                                            <field name="rate_packed_undone" widget="gauge" style="cursor: pointer;"/>
                                                        </span>
                                                    </div>
                                                    <div t-if="record.sga_integrated.raw_value == true">
                                                        <span class="o_label">
                                                           <field name="rate_sga_undone" widget="gauge" style="cursor: pointer;"/>
                                                        </span>
                                                    </div>
                                                    <div>
                                                        <span class="o_label">
                                                            <field name="rate_undone" widget="gauge" style="cursor: pointer;"/>
                                                        </span>
                                                    </div>
                                            </div>


                                        </div>
                                        <div t-if="record.visible_bargraph.raw_value == true" class="row">
                                             <field name="kanban_dashboard_graph" t-att-graph_type="'bar'" widget="dashboard_graph"/>
                                        </div>
                                    </div>


                                    <div class="container o_kanban_card_manage_pane">
                                        <div class="row">
                                            <div class="col-xs-6 o_kanban_card_manage_section o_kanban_manage_view">
                                                <div class="o_kanban_card_manage_title">
                                                    <span>Movimientos</span>
                                                </div>
                                                <div t-if="record.sga_integrated.raw_value == true" class="row">
                                                    <div class="col-xs-5 text-primary" title="Movimientos de SGA (Totales)">
                                                        <a name="get_action_tree" type="object" context="{'dest_model': 'stock.move',
                                                                                                          'move_domain': 'count_move_ready',
                                                                                                          'default_domain': [('sga_integrated', '=', True)],
                                                                                                          'search_default_waiting_sga': 1}">
                                                            Pendientes de SGA
                                                        </a>
                                                    </div>
                                                    <div class="col-xs-1">
                                                        <span><t t-esc="record.count_sga_undone.value"/> de <t t-esc="record.count_sga_send.value"/></span>
                                                    </div>
                                                </div>


                                                <div t-if="record.visible_count_move_ready.raw_value == true" class="row">
                                                    <div class="col-xs-5 text-primary" title="Movimientos que ya tienen reserva total o parcial y pueden ser procesados">
                                                        <a name="get_action_tree" type="object" context="{'dest_model': 'stock.move', 'move_domain': 'count_move_ready'}">
                                                            Preparados
                                                        </a>
                                                    </div>
                                                    <div class="col-xs-1">
                                                        <span><t t-esc="record.count_move_ready.value"/></span>
                                                    </div>
                                                </div>

                                                <div t-if="record.visible_count_move_unpacked.raw_value == true" class="row">
                                                    <div class="col-xs-5 text-primary" title="Para empaquetar  (Totales)">
                                                        <a name="get_action_tree" type="object" context="{'dest_model': 'stock.move',
                                                                                                          'move_domain': 'count_move_ready',
                                                                                                          'default_domain': [('sga_integrated', '=', True)],
                                                                                                          'search_default_waiting_sga': 1}">
                                                            Para empaquetar
                                                        </a>
                                                    </div>
                                                    <div class="col-xs-1">
                                                        <span><t t-esc="record.count_move_unpacked.value"/> de <t t-esc="record.count_move_ready.value"/></span>
                                                    </div>
                                                </div>

                                                <div t-if="record.visible_count_move_to_pick.raw_value == true" class="row">
                                                    <div class="col-xs-5 text-primary" title="Movimientos que no han sido asignados a un grupo">
                                                        <a name="get_action_tree" type="object" context="{'dest_model': 'stock.move',
                                                                                                          'move_domain': 'count_move_to_pick'}">
                                                            Para albaranar
                                                        </a>
                                                    </div>
                                                    <div class="col-xs-1">
                                                        <span><t t-esc="record.count_move_to_pick.value"/></span>
                                                    </div>
                                                </div>

                                                <div t-if="record.visible_count_move_backorders.raw_value == true" class="row">
                                                    <div class="col-xs-5 text-primary" title="Movimientos de retorno">
                                                        <a name="get_action_tree" type="object" context="{'dest_model': 'stock.move',
                                                                                                          'move_domain': 'count_move_backorders'}">
                                                            Back orders
                                                        </a>
                                                    </div>
                                                    <div  class="col-xs-1">
                                                        <span ><t t-esc="record.count_move_backorders.value"/></span>
                                                    </div>
                                                </div>

                                                <div t-if="record.visible_count_move_backorders.raw_value == true" class="row">
                                                    <div class="col-xs-5 text-primary" title="Todos los pasarán">
                                                        <a name="get_action_tree" type="object" context="{'dest_model': 'stock.move',
                                                                                                          'move_domain': 'count_move_pasaran'}">
                                                            Pasarán
                                                        </a>
                                                    </div>
                                                    <div class="col-xs-1">
                                                        <span><t t-esc="record.count_move_pasaran.value"/></span>
                                                    </div>
                                                </div>

                                            </div>
                                            <div class="col-xs-6 o_kanban_card_manage_section o_kanban_manage_view">
                                                <div class="o_kanban_card_manage_title">
                                                    <span>Otra info</span>
                                                </div>
                                            </div>
                                        </div>

                                        <div t-if="widget.editable" class="o_kanban_card_manage_settings row">
                                            <div class="col-xs-8">
                                                <ul class="oe_kanban_colorpicker" data-field="color"/>
                                            </div>
                                            <div class="col-xs-4 text-right">
                                                <a type="edit">Settings</a>
                                            </div>
                                        </div>
                                    </div>
